{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/(.*\\.(?:js|css|map|json|png|jpg|jpeg|svg|ico|woff2|woff|ttf))$",
      "dest": "/$1"
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
